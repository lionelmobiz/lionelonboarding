{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "getContactList/{clientId}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "contactsList",
      "databaseName": "practice-db",
      "collectionName": "contacts",
      "connectionStringSetting": "lionelonboarding_DOCUMENTDB",
      "sqlQuery": "SELECT * from c where c.clientId = {clientId}",
      "partitionKey": "clientId"
    }
  ],
  "scriptFile": "../dist/ListContacts/index.js"
}